<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.anderson.zaharov.entity.Employer" table="EMPLOYER">
        <meta attribute="class-description">
            This class contains the Employer details.
        </meta>
        <id name="id" type="int">
            <column name="id" precision="5" scale="0"/>
            <generator class="native"/>
        </id>
        <property name="name" type="string">
            <column name="name" length="30"/>
        </property>

        <property name="surName" type="string">
            <column name="surname" length="30"/>
        </property>

        <property name="patronymic" type="string">
            <column name="patronymic" length="30"/>
        </property>

        <property name="mail" type="string">
            <column name="mail" length="30"/>
        </property>

        <property name="tel" type="string">
            <column name="tel" length="30"/>
        </property>

        <property name="birthday" type="string">
            <column name="birthday" length="30"/>
        </property>

        <property name="experience" type="string">
            <column name="experience" length="30"/>
        </property>

        <property name="employmentDate" type="string">
            <column name="employment_data" length="30"/>
        </property>

        <property name="" type="string">
            <column name="" length="30"/>
        </property>

        <property name="" type="string">
            <column name="" length="30"/>
        </property>

        <property name="" type="string">
            <column name="" length="30"/>
        </property>

        <property name="" type="string">
            <column name="" length="30"/>
        </property>

        <property name="" type="string">
            <column name="" length="30"/>
        </property>

        <property name="" type="string">
            <column name="" length="30"/>
        </property>
    </class>
</hibernate-mapping>